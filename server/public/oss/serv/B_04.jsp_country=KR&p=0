<script type="text/JavaScript">
<!--
	var giftTrans = '';
	var titleId = '0001000146414651';
	var transId = giftsArray[titleId];
	
	if ((giftTrans != null) && (giftTrans != "")) {
		// list gifts only
		document.getElementById("giftTitle16").style.display = '';
	} else if (transId != null) {
		// has pending gift(s). B-24 should be displayed instead of B-05 for the pending gift titles
		document.getElementById("giftAnchor16").href = 'javascript:showGiftTrans('+transId+')';
		document.getElementById("storedGiftTitle16").style.display = '';
	} else {
		// no pending gift
		document.getElementById("regularTitle16").style.display = '';
		var titleInfoVar = ec.getTitleInfo ('0001000146414651');
		var latestVersion = parseInt('0');
		if (isTitleUpdateAvailable(titleInfoVar, latestVersion)) {
			document.getElementById('UPDATE16').style.display = ''; 
		}
		var titleLicense = getTitleLicense('0001000146414651');
        //trace("SCATitleStatus"+getSCATitleStatus());
		if (titleLicense == "Unlimited") {
			// Already own this title
			var len=document.getElementsByName('ImageB16').length;
			if (len >0) {
				for(j=0;j<len;j++) {
					document.getElementsByName('ImageB16')[j].onmouseover = 'MM_swapImage("'+ 'Image16' + '","","' + '/oss/oss/common/images/' + '/banner/B_04_softbannerP_b.gif",1); wiiFocusSound();'; 
				}
			}
	
			len=document.getElementsByName('ImageA16').length;
			if (len >0) {
				for(j=0;j<len;j++) {
				document.getElementsByName('ImageA16')[j].src = '/oss/oss/common/images//banner/B_04_softbannerP_a.gif'; 
				}
			}
	
			len=document.getElementsByName('purchasePoint16').length;
			if (!titleInfoVar.isOnDevice || isTitleUpdateAvailable(titleInfoVar, latestVersion)) {
				if (len >0) {
					for(j=0;j<len;j++) {
						document.getElementsByName('purchasePoint16')[j].innerHTML = '다운로드 가능'; 
					}
				}
			} else {
				if (len >0) {
					for(j=0;j<len;j++) {
						document.getElementsByName('purchasePoint16')[j].innerHTML = '다운로드 완료'; 
					}
				}
			}


            
            if ( "SCAGOLD" == getSCAUserStatus() || "SCASILVER" == getSCAUserStatus() ){ 
                if ( isScaGiftable(titleId) ) {
                    len=document.getElementsByName('purchasePoint16').length;
                    for(j=0;j<len;j++) {
                        document.getElementsByName('purchasePoint16')[j].innerHTML = '무료 선물'; 
