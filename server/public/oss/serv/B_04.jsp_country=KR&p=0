                    var sca_title_silver_status = '' ;

                    if ( "ACTIVATED" == getICRUserStatus() && "ICR" == icr_title_status ) {
                        len=document.getElementsByName('purchasePoint16').length;
                        for(j=0;j<len;j++) {
                            document.getElementsByName('purchasePoint16')[j].innerHTML = '(N/A)';
                        }
                    } else if ( "SCAGOLD" == getSCAUserStatus() && "SCAGOLD" == sca_title_gold_status )
                    {
                        len=document.getElementsByName('purchasePoint16').length;
                        for(j=0;j<len;j++) {
                            document.getElementsByName('purchasePoint16')[j].innerHTML = '스페셜 도우미20'; 
