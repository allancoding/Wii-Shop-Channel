					for(j=0;j<len;j++) {
						document.getElementsByName('purchasePoint28')[j].innerHTML = '다운로드 완료'; 
					}
				}
			}


            
            if ( "SCAGOLD" == getSCAUserStatus() || "SCASILVER" == getSCAUserStatus() ){ 
                if ( isScaGiftable(titleId) ) {
                    len=document.getElementsByName('purchasePoint28').length;
                    for(j=0;j<len;j++) {
                        document.getElementsByName('purchasePoint28')[j].innerHTML = '무료 선물'; 
                    }
                }
            }

            


		}else{
            
                    var icr_title_status        = '' ;
                    var sca_title_gold_status   = '' ; 
                    var sca_title_silver_status = '' ;

                    if ( "ACTIVATED" == getICRUserStatus() && "ICR" == icr_title_status ) {
                        len=document.getElementsByName('purchasePoint28').length;
                        for(j=0;j<len;j++) {
                            document.getElementsByName('purchasePoint28')[j].innerHTML = '(N/A)';
                        }
                    } else if ( "SCAGOLD" == getSCAUserStatus() && "SCAGOLD" == sca_title_gold_status )
                    {
                        len=document.getElementsByName('purchasePoint28').length;
                        for(j=0;j<len;j++) {
                            document.getElementsByName('purchasePoint28')[j].innerHTML = '스페셜 도우미20'; 
                        }
                    } else if ( "SCASILVER" == getSCAUserStatus() && "SCASILVER" == sca_title_silver_status )
                    {
                        len=document.getElementsByName('purchasePoint28').length;
                        for(j=0;j<len;j++) {
                            document.getElementsByName('purchasePoint28')[j].innerHTML = '스페셜 도우미10'; 
                        }
                    } 

                    
        }
	}
