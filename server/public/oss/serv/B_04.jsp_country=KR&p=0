		document.getElementById("storedGiftTitle25").style.display = '';
	} else {
		// no pending gift
		document.getElementById("regularTitle25").style.display = '';
		var titleInfoVar = ec.getTitleInfo ('000100014A415654');
		var latestVersion = parseInt('0');
		if (isTitleUpdateAvailable(titleInfoVar, latestVersion)) {
			document.getElementById('UPDATE25').style.display = ''; 
		}
		var titleLicense = getTitleLicense('000100014A415654');
        //trace("SCATitleStatus"+getSCATitleStatus());
		if (titleLicense == "Unlimited") {
			// Already own this title
			var len=document.getElementsByName('ImageB25').length;
			if (len >0) {
				for(j=0;j<len;j++) {
					document.getElementsByName('ImageB25')[j].onmouseover = 'MM_swapImage("'+ 'Image25' + '","","' + '/oss/oss/common/images/' + '/banner/B_04_softbannerP_b.gif",1); wiiFocusSound();'; 
				}
			}
	
			len=document.getElementsByName('ImageA25').length;
			if (len >0) {
				for(j=0;j<len;j++) {
				document.getElementsByName('ImageA25')[j].src = '/oss/oss/common/images//banner/B_04_softbannerP_a.gif'; 
				}
			}
	
			len=document.getElementsByName('purchasePoint25').length;
			if (!titleInfoVar.isOnDevice || isTitleUpdateAvailable(titleInfoVar, latestVersion)) {
				if (len >0) {
					for(j=0;j<len;j++) {
						document.getElementsByName('purchasePoint25')[j].innerHTML = '다운로드 가능'; 
					}
				}
			} else {
				if (len >0) {
					for(j=0;j<len;j++) {
						document.getElementsByName('purchasePoint25')[j].innerHTML = '다운로드 완료'; 
					}
				}
			}


            
            if ( "SCAGOLD" == getSCAUserStatus() || "SCASILVER" == getSCAUserStatus() ){ 
                if ( isScaGiftable(titleId) ) {
                    len=document.getElementsByName('purchasePoint25').length;
                    for(j=0;j<len;j++) {
                        document.getElementsByName('purchasePoint25')[j].innerHTML = '무료 선물'; 
                    }
                }
            }

            


		}else{
            
                    var icr_title_status        = '' ;
                    var sca_title_gold_status   = '' ; 
                    var sca_title_silver_status = '' ;

                    if ( "ACTIVATED" == getICRUserStatus() && "ICR" == icr_title_status ) {
                        len=document.getElementsByName('purchasePoint25').length;
                        for(j=0;j<len;j++) {
                            document.getElementsByName('purchasePoint25')[j].innerHTML = '(N/A)';
                        }
                    } else if ( "SCAGOLD" == getSCAUserStatus() && "SCAGOLD" == sca_title_gold_status )
                    {
                        len=document.getElementsByName('purchasePoint25').length;
                        for(j=0;j<len;j++) {
                            document.getElementsByName('purchasePoint25')[j].innerHTML = '스페셜 도우미20'; 
                        }
                    } else if ( "SCASILVER" == getSCAUserStatus() && "SCASILVER" == sca_title_silver_status )
                    {
                        len=document.getElementsByName('purchasePoint25').length;
                        for(j=0;j<len;j++) {
                            document.getElementsByName('purchasePoint25')[j].innerHTML = '스페셜 도우미10'; 
                        }
                    } 

                    
        }
	}
//-->
</script>

