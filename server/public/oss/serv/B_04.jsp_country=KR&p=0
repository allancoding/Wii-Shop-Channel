<script type="text/JavaScript">
<!--
	var giftTrans = '';
	var titleId = '000100014A425754';
	var transId = giftsArray[titleId];
	
	if ((giftTrans != null) && (giftTrans != "")) {
		// list gifts only
		document.getElementById("giftTitle35").style.display = '';
	} else if (transId != null) {
		// has pending gift(s). B-24 should be displayed instead of B-05 for the pending gift titles
		document.getElementById("giftAnchor35").href = 'javascript:showGiftTrans('+transId+')';
		document.getElementById("storedGiftTitle35").style.display = '';
	} else {
		// no pending gift
		document.getElementById("regularTitle35").style.display = '';
		var titleInfoVar = ec.getTitleInfo ('000100014A425754');
		var latestVersion = parseInt('0');
		if (isTitleUpdateAvailable(titleInfoVar, latestVersion)) {
			document.getElementById('UPDATE35').style.display = ''; 
		}
		var titleLicense = getTitleLicense('000100014A425754');
        //trace("SCATitleStatus"+getSCATitleStatus());
		if (titleLicense == "Unlimited") {
			// Already own this title
			var len=document.getElementsByName('ImageB35').length;
			if (len >0) {
				for(j=0;j<len;j++) {
					document.getElementsByName('ImageB35')[j].onmouseover = 'MM_swapImage("'+ 'Image35' + '","","' + '/oss/oss/common/images/' + '/banner/B_04_softbannerP_b.gif",1); wiiFocusSound();'; 
				}
			}
	
			len=document.getElementsByName('ImageA35').length;
			if (len >0) {
				for(j=0;j<len;j++) {
				document.getElementsByName('ImageA35')[j].src = '/oss/oss/common/images//banner/B_04_softbannerP_a.gif'; 
				}
			}
	
			len=document.getElementsByName('purchasePoint35').length;
			if (!titleInfoVar.isOnDevice || isTitleUpdateAvailable(titleInfoVar, latestVersion)) {
				if (len >0) {
					for(j=0;j<len;j++) {
						document.getElementsByName('purchasePoint35')[j].innerHTML = '다운로드 가능'; 
					}
				}
			} else {
				if (len >0) {
					for(j=0;j<len;j++) {
						document.getElementsByName('purchasePoint35')[j].innerHTML = '다운로드 완료'; 
					}
				}
			}


            
            if ( "SCAGOLD" == getSCAUserStatus() || "SCASILVER" == getSCAUserStatus() ){ 
                if ( isScaGiftable(titleId) ) {
                    len=document.getElementsByName('purchasePoint35').length;
                    for(j=0;j<len;j++) {
                        document.getElementsByName('purchasePoint35')[j].innerHTML = '무료 선물'; 
                    }
                }
            }

            


		}else{
            
                    var icr_title_status        = '' ;
                    var sca_title_gold_status   = '' ; 
                    var sca_title_silver_status = '' ;

                    if ( "ACTIVATED" == getICRUserStatus() && "ICR" == icr_title_status ) {
                        len=document.getElementsByName('purchasePoint35').length;
                        for(j=0;j<len;j++) {
                            document.getElementsByName('purchasePoint35')[j].innerHTML = '(N/A)';
                        }
                    } else if ( "SCAGOLD" == getSCAUserStatus() && "SCAGOLD" == sca_title_gold_status )
                    {
                        len=document.getElementsByName('purchasePoint35').length;
                        for(j=0;j<len;j++) {
                            document.getElementsByName('purchasePoint35')[j].innerHTML = '스페셜 도우미20'; 
                        }
                    } else if ( "SCASILVER" == getSCAUserStatus() && "SCASILVER" == sca_title_silver_status )
                    {
                        len=document.getElementsByName('purchasePoint35').length;
                        for(j=0;j<len;j++) {
                            document.getElementsByName('purchasePoint35')[j].innerHTML = '스페셜 도우미10'; 
                        }
                    } 

                    
        }
	}
//-->
