<script type="text/javascript">
function pageDirect()
{
    var qs = new Querystring();
    var p = qs.get('p');

    var pageName = {HYOKA_KIJUN:17};
    var pageNo = pageName[p];
    if (pageNo == null) {
        pageNo = p;
    }

    if (pageNo != '' && pageNo != null && isNaN(pageNo) == false) {
        jumpPage(pageNo);
    }
}
</script>
